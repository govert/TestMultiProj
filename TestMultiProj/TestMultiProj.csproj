<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ExcelDna.AddIn" Version="1.8.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MoreFunctions\MoreFunctions.csproj" />
  </ItemGroup>

  <PropertyGroup>
    <!-- Comma-separated list of additional add-in libraries to add as 'ExternalLibrary' entries in the generated .dna file -->
    <ExcelAddInExports>MoreFunctions.dll</ExcelAddInExports>
  </PropertyGroup>

</Project>
